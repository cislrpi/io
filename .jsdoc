{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "src/",
        "includePattern": ".js"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": ".ignore/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}