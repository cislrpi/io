{
  "name": "celio",
  "version": "2.2.0",
  "description": "A framework for building distributed, multimodal applications",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm install && npm prune",
    "lint": "eslint src/",
    "docs": "jsdoc -c .jsdoc",
    "compile": "babel src/ --out-dir lib/",
    "prepublish": "npm run compile",
    "preversion": "npm test",
    "version": "git add package.json",
    "pretest-browser": "browserify test/browser/index.js -t babelify -o test/browser/page/tests-bundle.js",
    "test-browser": "testem",
    "posttest-browser": "rm test/browser/page/tests-bundle.js",
    "test-node": "mocha test/node",
    "test": "npm run lint && npm run test-node && npm run test-browser",
    "update-gh-pages": "git checkout gh-pages && git pull origin gh-pages && cp -Rf .ignore/ . && rm -r .ignore/",
    "release-patch": "false",
    "release-minor": "npm version minor",
    "release-major": "npm version major"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "contributors": [
    {
      "name": "Yedendra Shrinivasan",
      "email": "yshrini@us.ibm.com"
    },
    {
      "name": "Yunfeng Zhang",
      "email": "zhangyun@us.ibm.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.ibm.com/celio/CELIO.git"
  },
  "publishConfig": {
    "registry": "https://cel-npm-registry.mybluemix.net/"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "amqplib": "^0.4.2",
    "bluebird": "^3.4.6",
    "lodash": "^4.13.1",
    "nconf": "^0.8.4",
    "redis": "^2.6.2",
    "stompjs": "^2.3.3",
    "three": "^0.78.0",
    "uuid": "^2.0.2",
    "webdismay": "^0.9.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-eventemitter": "^1.1.1",
    "docdash": "^0.4.0",
    "jsdoc": "^3.4.2",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "testem": "^1.13.0"
  }
}
